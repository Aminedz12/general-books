  $(y+3)(y+2) = (y-1)$

\begin{tabular}{l@{\qquad\quad}l}
(a) $x > y$ & (b) $x < y$\\ 
(c) $x \leq y$ & (d) $x \geq y$\\ 
\multicolumn{2}{l}{(e) $x =y0$ or relation cannot be established}
\end{tabular}

\item $12x^2 + 29x + 14 = 0$

$y^2 + 9y + 18 = 0$

\begin{tabular}{l@{\qquad\quad}l}
(a) $x > y$ & (b) $x < y$\\ 
(c) $x \leq y$ & (d) $x \geq y$\\ 
\multicolumn{2}{l}{(e) $x =y0$ or relation cannot be established}
\end{tabular}

\item $5x^2 - 18x + 9 = 0$

  $3y^2 + 5y - 2 = 0$


\begin{tabular}{l@{\qquad\quad}l}
(a) $x > y$ & (b) $x < y$\\ 
(c) $x \leq y$ & (d) $x \geq y$\\ 
\multicolumn{2}{l}{(e) $x =y0$ or relation cannot be established}
\end{tabular}
\item $17^2 + 144 \div 18 = x$

  $26^2 - 18x \times 21 = y$

\begin{tabular}{l@{\qquad\quad}l}
(a) $x > y$ & (b) $x < y$\\ 
(c) $x \leq y$ & (d) $x \geq y$\\ 
\multicolumn{2}{l}{(e) $x =y0$ or relation cannot be established}
\end{tabular}

\item $30x^2 + 11x + 1 = 0$

  $42y^2 + 13y + 1 = 0$

\begin{tabular}{l@{\qquad\quad}l}
(a) $x > y$ & (b) $x < y$\\ 
(c) $x \leq y$ & (d) $x \geq y$\\ 
\multicolumn{2}{l}{(e) $x =y0$ or relation cannot be established}
\end{tabular}
