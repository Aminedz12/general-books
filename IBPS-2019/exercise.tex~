      $P^2 + 4P - P - 4 = 0$\\
      $\Rightarrow\quad P(P + 4) -1(P + 4) = 0$\\
      $\Rightarrow\quad P = 1, -4$\\

    \item[{\bf II.}] $3Q^2 - 10Q + 8 = 0$\\
      $3Q^2 - 6Q - 4Q + 8 = 0$\\
      $3Q(Q - 2) -4(Q - 2) = 0$\\
      $(3Q - 4)(Q - 2) = 0$\\
      $\Rightarrow\quad Q = 4/3, 2$\\
      $\therefore~ Q > P$
      \end{itemize}
  \end{itemize}
