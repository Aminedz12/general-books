  \item[(c)]
    \begin{itemize}
    \item[{\bf I.}] $4P^2 - 8P + 3 = 0$\\
      $4P^2 - 2P - 6P + 3 = 0$\\
      $2P(2P - 1)-3 (2P - 1) = 0$\\
      $(2P - 3)(2P - 1) = 0$\\
      $\Rightarrow\quad P = 1/2. 3/2$

    \item[{\bf II.}] $2Q^2 - 13A + 15 = 0$\\
      $2Q^2 - 10Q - 3Q + 15 = 0$\\
      $2Q(Q - 5) -3(Q - 5) = 0$\\
      $\Rightarrow\quad (2Q - 3)(Q - 5) = 0$\\
      $\Rightarrow\quad Q = 3/2, 5$\\
      $\therefore\ Q \geq P$
      \end{itemize}
  \end{itemize}
